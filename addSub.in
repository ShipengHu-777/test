procedure main () returns integer {
    var out = std_output();

    integer i1 = 10;
    integer i2 = 20;

    real r1 = 1.34e5;
    real r2 = 34.1e4;

    (i1 + i2) -> out;
    '\n' -> out;
    (i1 - i2) -> out;
    '\n' -> out;
    (i2 - i1) -> out;
    '\n' -> out;

    '\n' -> out;
    (r1 + r2) -> out;
    '\n' -> out;
    (r1 - r2) -> out;
    '\n' -> out;
    (r2 - r1) -> out;
    '\n' -> out;

    return 0;
}